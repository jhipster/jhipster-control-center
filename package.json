    "husky": "7.0.2",
    "jest": "27.3.1",
    "jest-junit": "13.0.0",
    "jest-serializer-vue": "2.0.2",
    "jest-sonar-reporter": "2.0.0",
    "jest-vue-preprocessor": "1.7.1",
    "lighthouse": "8.5.1",
    "lint-staged": "11.2.3",
