    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.3.1",
    "webpack-merge": "5.8.0",
    "workbox-webpack-plugin": "6.3.0"
  },
  "engines": {
    "node": ">=14.17.6",
    "npm": ">= 6.14.4"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
